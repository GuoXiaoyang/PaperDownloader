{
  "manifest_version": 2,
  "name": "IEEE Downloader",
  "version": "1.0",
  "description": "下载论文",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
/*      "css": [
        "css/mystyles.css"
      ],*/
      "js": [
        "js/jquery.js",
        "js/cnkidownload.js"
        //"js/download.js"

      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "scripts": [
      "js/background.js",
      "js/jquery.js",
      "js/cnkiDownload.js"

    ]
  },
  "browser_action": {
    "default_icon": {
      "32": "images/icon32.png"
    },
    "default_title": "Paper Downloader",
    "default_popup": "popup.html"
  },
  //    "options_page": "options.html",
  "permissions": [
    "<all_urls>",
    "webNavigation",
    "contextMenus",
    "tabs",
    "downloads"
  ]
}